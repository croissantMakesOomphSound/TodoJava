      <Button layoutX="300.0" layoutY="99.0" mnemonicParsing="false" onAction="#UndoButton" text="Undo" />
      <Button layoutX="353.0" layoutY="99.0" mnemonicParsing="false" onAction="#FinishButton" text="Finish" />
      <Label fx:id="notification" layoutX="14.0" layoutY="96.0" prefHeight="18.0" prefWidth="202.0" textFill="#f20505">
         <font>
            <Font size="10.0" />
